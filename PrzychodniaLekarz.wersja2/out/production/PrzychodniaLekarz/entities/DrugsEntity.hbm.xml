<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.DrugsEntity" table="drugs" schema="ak_db5">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="drugName">
            <column name="drug_name" sql-type="varchar(50)"/>
        </property>
        <property name="price">
            <column name="price" sql-type="float" precision="-1"/>
        </property>
        <property name="drugGroupId">
            <column name="drug_group_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>